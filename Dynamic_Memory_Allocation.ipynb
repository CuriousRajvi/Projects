    "if __name__=='__main__':\n",
    "    print('--------------------------------------------')\n",
    "    print('    Dynamic Memory Management Algorithms    ')\n",
    "    print('--------------------------------------------')\n",
    "    \n",
    "    Flag=True\n",
    "    while Flag:\n",
    "        #Inputting process number and memory requirement\n",
    "        print('\\nProcesses: ')\n",
    "        print('--------------------------------------------')\n",
    "    \n",
    "        ps=int(input(\"Input number of processes: \"))\n",
    "        print('--------------------------------------------')\n",
    "    \n",
    "        processes=[int(input(f\"Enter memory requirements of process {i} (in KB/s): \")) for i in range(ps)]\n",
    "        print('--------------------------------------------')\n",
    "    \n",
    "        #Input number of empty holes and hole size\n",
    "        print('\\nEmpty holes: ')\n",
    "        print('--------------------------------------------')\n",
    "        bs=int(input(\"Input number of empty holes in order: \"))\n",
    "        blocks=[int(input(f\"Enter hole size {i} (in KB/s): \")) for i in range(bs) ]\n",
    "        print(\"--------------------------------------------\")\n",
    "        print(\"--------------------------------------------\")  \n",
    "    \n",
    "  \n",
    "        #choose which algorithm to fit\n",
    "        print(\"1. First Fit \")\n",
    "        print(\"2. Next Fit \")    \n",
    "        print(\"3. Best Fit \")\n",
    "        print(\"4. Worst Fit \")\n",
    "        choice=int(input(\"Enter choice: \"))\n",
    "        print('\\n')\n",
    "        \n",
    "        if choice==1:\n",
    "\n",
    "            firstFit(processes,ps,blocks,bs)\n",
    "        \n",
    "        elif choice==2:\n",
    "\n",
    "            nextFit(processes,ps,blocks,bs)\n",
    "        \n",
    "        elif choice==3:\n",
    "\n",
    "            bestFit(processes,ps,blocks,bs)\n",
    "        \n",
    "        elif choice==4:\n",
    "\n",
    "            worstFit(processes,ps,blocks,bs)\n",
    "        \n",
    "        else:\n",
    "            print(\"Invalid option! \")\n",
    "        \n",
    "        print(\"\\n\")\n",
    "        \n",
    "        c=input('Want to continue ? y/n  ')\n",
    "        \n",
    "        if c=='y' or c=='Y':\n",
    "            Flag=True\n",
    "        else:\n",
    "            Flag=False\n",
    "            \n",
    "#contributed by rajvi desai"
   ]
